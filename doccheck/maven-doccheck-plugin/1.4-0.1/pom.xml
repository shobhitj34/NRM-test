<project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>doccheck</groupId>
  <artifactId>maven-doccheck-plugin</artifactId>
  <name>Maven DocCheck Plug-in</name>
  <version>1.4-0.1</version>
  <url>http://maven-doccheck.sourceforge.net/</url>
  <inceptionYear>2004</inceptionYear>
  <mailingLists>
    <mailingList>
      <name>&lt;a href="http://lists.sourceforge.net/lists/listinfo/maven-doccheck-user">Maven-DocCheck User List&lt;/a></name>
      <archive>http://sourceforge.net/mailarchive/forum.php?forum=maven-doccheck-user</archive>
    </mailingList>
  </mailingLists>
  <developers>
    <developer>
      <id>shinobukawai</id>
      <name>Shinobu Kawai</name>
      <email>shinobu@ieee.org</email>
      <url>http://sylow.no-ip.com/maven/</url>
      <organization>org.ieee.shinobu</organization>
      <roles>
        <role>Java Developer</role>
      </roles>
      <timezone>+9</timezone>
    </developer>
  </developers>
  <dependencies>
    <dependency>
      <groupId>doccheck</groupId>
      <artifactId>doccheck</artifactId>
      <version>1.2b2</version>
    </dependency>
  </dependencies>
  <reports>
    <plugins>
      <plugin>
        <groupId>maven</groupId>
        <artifactId>changes</artifactId>
        <version>1.0-SNAPSHOT</version>
        <goals>
          <goal>
            <id>report</id>
          </goal>
        </goals>
      </plugin>
      <plugin>
        <groupId>maven</groupId>
        <artifactId>changelog</artifactId>
        <version>1.0-SNAPSHOT</version>
        <goals>
          <goal>
            <id>report</id>
          </goal>
        </goals>
      </plugin>
      <plugin>
        <groupId>maven</groupId>
        <artifactId>developer-activity</artifactId>
        <version>1.0-SNAPSHOT</version>
        <goals>
          <goal>
            <id>report</id>
          </goal>
        </goals>
      </plugin>
      <plugin>
        <groupId>maven</groupId>
        <artifactId>file-activity</artifactId>
        <version>1.0-SNAPSHOT</version>
        <goals>
          <goal>
            <id>report</id>
          </goal>
        </goals>
      </plugin>
      <plugin>
        <groupId>maven</groupId>
        <artifactId>license</artifactId>
        <version>1.0-SNAPSHOT</version>
        <goals>
          <goal>
            <id>report</id>
          </goal>
        </goals>
      </plugin>
      <plugin>
        <groupId>maven</groupId>
        <artifactId>linkcheck</artifactId>
        <version>1.0-SNAPSHOT</version>
        <goals>
          <goal>
            <id>report</id>
          </goal>
        </goals>
      </plugin>
      <plugin>
        <groupId>maven</groupId>
        <artifactId>faq</artifactId>
        <version>1.0-SNAPSHOT</version>
        <goals>
          <goal>
            <id>report</id>
          </goal>
        </goals>
      </plugin>
    </plugins>
  </reports>
  <scm>
    <connection>scm:cvs:pserver:anonymous@cvs.sourceforge.net:/cvsroot/maven-doccheck:maven-doccheck-plugin</connection>
    <url>http://cvs.sourceforge.net/viewcvs.py/maven-doccheck/maven-doccheck-plugin</url>
  </scm>
  <build>
    <sourceDirectory></sourceDirectory>
    <testSourceDirectory></testSourceDirectory>
    <resources>
      <resource>
        <directory>.</directory>
        <includes>
          <include>plugin.jelly</include>
          <include>plugin.properties</include>
          <include>project.properties</include>
          <include>project.xml</include>
        </includes>
      </resource>
    </resources>
    <plugins>
      <plugin>
        <groupId>maven</groupId>
        <artifactId>maven-surefire-plugin</artifactId>
        <version>1.0-SNAPSHOT</version>
        <configuration>
          <excludes>
            <exclude>**/*~</exclude>
            <exclude>**/#*#</exclude>
            <exclude>**/.#*</exclude>
            <exclude>**/%*%</exclude>
            <exclude>**/._*</exclude>
            <exclude>**/CVS</exclude>
            <exclude>**/CVS/**</exclude>
            <exclude>**/.cvsignore</exclude>
            <exclude>**/SCCS</exclude>
            <exclude>**/SCCS/**</exclude>
            <exclude>**/vssver.scc</exclude>
            <exclude>**/.svn</exclude>
            <exclude>**/.svn/**</exclude>
            <exclude>**/.DS_Store</exclude>
          </excludes>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <organization>
    <name>org.ieee.shinobu</name>
    <url>http://sourceforge.net/projects/maven-doccheck</url>
  </organization>
  <distributionManagement>
    <site>
      <id>default</id>
      <name>Default Site</name>
      <url>scp://shell.sourceforge.net//home/groups/m/ma/maven-doccheck/htdocs</url>
    </site>
  </distributionManagement>
</project>